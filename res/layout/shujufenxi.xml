<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fcdef3"
    android:weightSum="1">

    <LinearLayout android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_marginTop="10dp"
        android:background="#f8f8f8"
        android:layout_weight="0.04"
        android:weightSum="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            android:weightSum="1"
            android:layout_weight="0.06">

            <LinearLayout
                android:layout_width="195dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:text="年龄结构"
                    android:textSize="21dp"
                    android:layout_marginLeft="20dp"/>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="3dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="20岁以下"
                        android:textSize="17dp"
                        android:layout_marginLeft="3dp"/>

                    <ProgressBar
                        android:id="@+id/pbHor1"
                        style="@android:style/Widget.ProgressBar.Horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:max="100"
                        android:progress="10"
                        android:progressDrawable="@drawable/processbar"
                        />

                </LinearLayout>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="4dp"
                    android:weightSum="1">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="21—25岁"
                        android:textSize="17dp"
                        android:layout_marginLeft="3dp" />

                    <ProgressBar
                        android:id="@+id/pbHor2"
                        style="@android:style/Widget.ProgressBar.Horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:max="100"
                        android:progress="34"
                        android:progressDrawable="@drawable/processbar"
                        />

                </LinearLayout>
                <LinearLayout android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="4dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="26—30岁"
                    android:textSize="17dp"
                    android:layout_marginLeft="3dp"/>

                <ProgressBar
                    android:id="@+id/pbHor3"
                    style="@android:style/Widget.ProgressBar.Horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:max="100"
                    android:progress="50"
                    android:progressDrawable="@drawable/processbar"
                    />

            </LinearLayout>
            <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="4dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="31—39岁"
                        android:textSize="17dp"
                        android:layout_marginLeft="3dp"/>

                    <ProgressBar
                        android:id="@+id/pbHor4"
                        style="@android:style/Widget.ProgressBar.Horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:max="100"
                        android:progress="6"
                        android:progressDrawable="@drawable/processbar"
                        />

            </LinearLayout>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="4dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="40岁以上"
                        android:textSize="17dp"
                        android:layout_marginLeft="3dp"/>

                    <ProgressBar
                        android:id="@+id/pbHor5"
                        style="@android:style/Widget.ProgressBar.Horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:max="100"
                        android:progress="0"
                        android:progressDrawable="@drawable/processbar"
                        />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="147dp"
                android:orientation="vertical"
                android:weightSum="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:text="肤质比例"
                    android:textSize="21dp"
                    android:layout_marginLeft="20dp"/>
                <LinearLayout
                    android:layout_width="242dp"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:weightSum="1">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_marginTop="1dp">

                        <ProgressBar
                            android:id="@+id/pbHor6"
                            android:layout_width="19dp"
                            android:layout_height="104dp"
                            android:layout_marginLeft="10dp"
                            android:indeterminateOnly="false"
                            android:max="100"
                            android:progress="10"
                            android:progressDrawable="@drawable/vertical_progressbar"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="干性"
                            android:textSize="15dp"
                            android:layout_marginLeft="5dp"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="34dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_marginTop="3dp">

                        <ProgressBar
                            android:id="@+id/pbHor7"
                            android:layout_width="19dp"
                            android:layout_height="102dp"
                            android:layout_marginLeft="8dp"
                            android:indeterminateOnly="false"
                            android:max="100"
                            android:progress="13"
                            android:progressDrawable="@drawable/vertical_progressbar"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="油性"
                            android:textSize="15dp"
                            android:layout_marginLeft="3dp"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="32dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_marginTop="3dp"
                        android:layout_weight="0.06">

                        <ProgressBar
                            android:id="@+id/pbHor8"
                            android:layout_width="19dp"
                            android:layout_height="102dp"
                            android:layout_marginLeft="8dp"
                            android:indeterminateOnly="false"
                            android:max="100"
                            android:progress="12"
                            android:progressDrawable="@drawable/vertical_progressbar"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="中性"
                            android:textSize="15dp"
                            android:layout_marginLeft="3dp"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="33dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_marginTop="3dp">

                        <ProgressBar
                            android:id="@+id/pbHor9"
                            android:layout_width="19dp"
                            android:layout_height="102dp"
                            android:layout_marginLeft="4dp"
                            android:indeterminateOnly="false"
                            android:max="100"
                            android:progress="48"
                            android:progressDrawable="@drawable/vertical_progressbar"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="混合"
                            android:textSize="15dp"
                            android:layout_marginLeft="0dp"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_marginTop="3dp">

                        <ProgressBar
                            android:id="@+id/pbHor10"
                            android:layout_width="19dp"
                            android:layout_height="102dp"
                            android:layout_marginLeft="9dp"
                            android:indeterminateOnly="false"
                            android:max="100"
                            android:progress="17"
                            android:progressDrawable="@drawable/vertical_progressbar"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="敏感肌"
                            android:textSize="15dp"
                            android:layout_marginLeft="0dp"/>

                    </LinearLayout>


                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="0.53"
            android:weightSum="1">
            <TextView android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_gravity="left"
                android:layout_marginTop="3dp"
                android:text="综合评分"
                android:textSize="21dp"
                android:layout_marginLeft="20dp"/>
            <LinearLayout android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="end"
                    android:text="4.0"
                    android:textSize="35dp"
                    android:textColor="#ff4081"
                    android:id="@+id/textView"
                    android:layout_marginLeft="160dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="/5.0"
                    android:textSize="17dp"
                    android:textColor="#c9c9c9"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal">
            <ImageView android:layout_height="19dp"
                android:layout_width="23dp"
                android:src="@drawable/star_full_16px_1129028_easyicon"
                android:layout_marginLeft="140dp"
                />
                <ImageView android:layout_height="19dp"
                    android:layout_width="23dp"
                    android:src="@drawable/star_full_16px_1129028_easyicon"
                    android:layout_marginLeft="0dp"
                    />
                <ImageView android:layout_height="19dp"
                    android:layout_width="23dp"
                    android:src="@drawable/star_full_16px_1129028_easyicon"
                    android:layout_marginLeft="0dp"
                    />
                <ImageView android:layout_height="19dp"
                    android:layout_width="23dp"
                    android:src="@drawable/star_full_16px_1129028_easyicon"
                    android:layout_marginLeft="0dp"
                    />
                <ImageView android:layout_height="19dp"
                    android:layout_width="23dp"
                    android:src="@drawable/star_empty_16px_1129027_easyicon"
                    android:layout_marginLeft="0dp"
                    />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>